<%- include('main/head'); -%>
<%- include('main/sidebar'); -%>


   <!-- **********************************************************************************************************************************************************
      MAIN CONTENT
      *********************************************************************************************************************************************************** -->
      <!--main content start-->
      
      <section id="main-content">
        <section class="wrapper">
            <h3><i class="fa fa-angle-right"></i> Brand List</h3>
              <div class="row">
            

            <div class="row mt">
                <div class="col-md-12">
                    <div class="content-panel">
                        <table class="table table-striped table-advance table-hover">
                                
                                <hr>
                            <thead>
                            <tr>
                                <th><i class="fa fa-bullhorn"></i> Brand ID</th>
                                <th class="hidden-phone"><i class="fa fa-question-circle"></i>Brand Name</th>
                                <th><i class="fa fa-bookmark"></i> Brand Modelcode</th>
                                <th><i class="fa fa-edit"></i>Status</th>
                                <th></th>
                            </tr>
                            </thead>
                            <tbody>
                                <% if (data && data.brlist){%>
                                    <% for(var i = 0; i < data.brlist.length; i++){ %>
                                <tr>
                                    
                                    <td><%= data.brlist[i].brid %></td>
                                    <td><%= data.brlist[i].brname %> </td>
                                    <td><%= data.brlist[i].brmodcode %></td>
                                   
                                              
                                   
                                   <td>
                                    <% if ( data.brlist[i].status < 1){%>
                                            <a class="button_1" href="/admin/updateprdt/<%=data.brlist[i].brid %>">  
                                            <button class="btn btn-success btn-xs"><i class="fa fa-edit"></i></button></a>
                                            <% } else{ %> 
                                                Updated
                                             <% } %>
                                            
        
                                       
                                    </td>
                                </tr>
                                  
                                <% } %>
                                <% } %>
                           
                            
                            
                            </tbody>
                        </table>
                    </div><!-- /content-panel -->
                </div><!-- /col-md-12 -->
            
            </div><!-- /row -->

      </section><! --/wrapper -->
    </section><!-- /MAIN CONTENT -->
</form>


<%- include('main/foot'); -%>   
      