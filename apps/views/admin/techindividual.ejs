<%- include('main/head'); -%>
 <!-- Favicons -->
 <link href="../img/favicon.png" rel="icon">
 <link href="../img/apple-touch-icon.png" rel="apple-touch-icon">

 <!-- Bootstrap core CSS -->
 <link href="../lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
 <!--external css-->
 <link href="../lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
 <link rel="stylesheet" type="text/css" href="../css/zabuto_calendar.css">
 <link rel="stylesheet" type="text/css" href="../lib/gritter/css/jquery.gritter.css" />
 <!-- Custom styles for this template -->
 <link href="../css/style.css" rel="stylesheet">
 <link href="../css/style-responsive.css" rel="stylesheet">
 <script src="../lib/chart-master/Chart.js"></script>
<%- include('main/sidebar'); -%>
    

<!--main content start-->
<section id="main-content">
    <section class="wrapper">
      <h3><i class="fa fa-angle-right"></i> Approval of Technician Form Validation</h3>
<div class="row mt">
    <div class="col-lg-12">
      <h4><i class="fa fa-angle-right"></i> Technician Form Validations</h4>
      <div class="form-panel">
        <div class="form">
          <% if (data && data.techn){%>
          <form class="cmxform form-horizontal style-form" id="signupForm" method="POST" action="/admin/techindividual">
            <div class="form-group ">
              <label for="id" class="control-label col-lg-2">Technician Id</label>
              <div class="col-lg-10">
             <input class=" form-control" id="techid" name="techid"  type="text" value="<%= data.techn[0].id %> "/>
              </div>
              </div>
             
            <div class="form-group ">
              <label for="firstname" class="control-label col-lg-2">Firstname</label>
              <div class="col-lg-10">
                <input class=" form-control" id="firstname" name="firstname"  type="text" value="<%= data.techn[0].first_name %> "/>
              </div>
              </div>
        
            <div class="form-group ">
              <label for="lastname" class="control-label col-lg-2">Lastname</label>
              <div class="col-lg-10">
                <input class=" form-control" id="lastname" name="lastname"  type="text" value="<%= data.techn[0].last_name %> "/>
              </div>
            </div>
            <div class="form-group ">
              <label for="username" class="control-label col-lg-2">Date of Birth</label>
              <div class="col-lg-10">
                <input class="form-control " id="dob"  name="dob"   type="text"  value="<%= data.techn[0].dob %> "/>
              </div>
            </div>
            <div class="form-group ">
              <label for="username" class="control-label col-lg-2">Age</label>
              <div class="col-lg-10">
                <input class="form-control " id="age"  name="age"   type="text"  value="<%= data.techn[0].age %> "/>
              </div>
            </div>
            <div class="form-group ">
              <label for="password" class="control-label col-lg-2">Qualification</label>
              <div class="col-lg-10">
                <input class="form-control " id="quali" name="quali" type="text"value="<%= data.techn[0].quali %> "/>
              </div>
            </div>
            <div class="form-group ">
              <label for="confirm_password" class="control-label col-lg-2">Experience</label>
              <div class="col-lg-10">
                <input class="form-control " id="exp" name="exp" type="text" value="<%= data.techn[0].exp %> "/>
              </div>
            </div>
            <div class="form-group ">
              <label for="email" class="control-label col-lg-2">Email ID</label>
              <div class="col-lg-10">
                <input class="form-control " id="email" name="email" type="email" value="<%= data.techn[0].email %> " />
              </div>
            </div>
            <div class="form-group ">
              <label for="email" class="control-label col-lg-2">Mobile Number</label>
              <div class="col-lg-10">
                <input class="form-control " id="mob" name="mob" type="text" value="<%= data.techn[0].mobno %> "/>
              </div>
            </div>
            
            <div class="form-group ">
              <label for="agree" class="control-label col-lg-2 col-sm-3">Approved your application</label>
              <div class="col-lg-10 col-sm-9">
                <input type="checkbox" style="width: 20px" class="checkbox form-control" id="agree" name="agree" />
              </div>
            </div>
            <div class="form-group ">
              <label for="newsletter" class="control-label col-lg-2 col-sm-3">Cancel your application</label>
              <div class="col-lg-10 col-sm-9">
                <input type="checkbox" style="width: 20px" class="checkbox form-control" id="newsletter" name="newsletter" />
              </div>
            </div>
            <div class="form-group">
              <div class="col-lg-offset-2 col-lg-10">
                <a class="button_1" href="/admin/techindividual">
                <button class="btn btn-theme" type="submit">Save</button></a>
                <button class="btn btn-theme04" type="button">Cancel</button>
              </div>
            </div>
          </form>
          <%}%>
        </div>
      </div>
      <!-- /form-panel -->
    </div>
    <!-- /col-lg-12 -->
  </div>
  <!-- /row -->
  </section>
</section>
<%- include('main/foot'); -%>