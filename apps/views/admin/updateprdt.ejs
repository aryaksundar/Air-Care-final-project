<%- include('main/head'); -%>
 <!-- Favicons -->
 <link href="../img/favicon.png" rel="icon">
 <link href="../img/apple-touch-icon.png" rel="apple-touch-icon">

 <!-- Bootstrap core CSS -->
 <link href="../lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
 <!--external css-->
 <link href="../lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
 <link rel="stylesheet" type="text/css" href="../css/zabuto_calendar.css">
 <link rel="stylesheet" type="text/css" href="../lib/gritter/css/jquery.gritter.css" />
 <!-- Custom styles for this template -->
 <link href="../css/style.css" rel="stylesheet">
 <link href="../css/style-responsive.css" rel="stylesheet">
 <script src="../lib/chart-master/Chart.js"></script>

<%- include('main/sidebar'); -%>
<section id="main-content">
  <section class="wrapper">
    <h3><i class="fa fa-angle-right"></i> Update Brands</h3>
<div class="row mt">
    <div class="col-lg-12">
      <h4><i class="fa fa-angle-right"></i> Update Brand</h4>
      <div class="form-panel">
        <div class="form">
          <% if (data && data.brandind){%>
          <form class="cmxform form-horizontal style-form" id="signupForm" method="POST" action="/admin/updateprdt">
            <div class="form-group ">
              <label for="Brand_Id" class="control-label col-lg-2">Brand_Id</label>
              <div class="col-lg-10">
                <input class=" form-control" id="bid" name="bid" type="number"  value="<%=data.brandind[0].brid%>"/>
              </div>
            </div>
            <div class="form-group ">
              <label for="Brand_Name" class="control-label col-lg-2">Brand_Name</label>
              <div class="col-lg-10">
                <input class=" form-control" id="bname" name="bname" type="text" value="<%=data.brandind[0].brname%>" />
              </div>
            </div>
            <div class="form-group ">
              <label for="Brand_modelcode" class="control-label col-lg-2">Brand_modelcode</label>
              <div class="col-lg-10">
                <input class="form-control " id="mcode" name="mcode" type="text" value="<%=data.brandind[0].brmodcode%>"/>
              </div>
            </div>
            <div class="form-group ">
              <label for="Units_in_Stock" class="control-label col-lg-2">Units_in_Stock</label>
              <div class="col-lg-10">
                <input class="form-control " id="ustock" name="ustock" type="number"  value="<%=data.brandind[0].unitinstock%>" />
              </div>
            </div>
            <div class="form-group ">
              <label for="sold" class="control-label col-lg-2">In_Sold</label>
              <div class="col-lg-10">
                <input class="form-control " id="sold" name="sold" type="number" value="<%=data.brandind[0].insold%>"/>
              </div>
            </div>

            <div class="form-group ">
              <label for="Brand_Price" class="control-label col-lg-2">Brand_Price/unit</label>
              <div class="col-lg-10">
                <input class="form-control " id="bprice" name="bprice" type="number" value="<%=data.brandind[0].brperunit%>" />
              </div>
            </div>
            <div class="form-group ">
              <label for="ac_Type" class="control-label col-lg-2">AC Type</label>
              <div class="col-lg-10">
                <input class="form-control " id="ac_type" name="parent_category_name" type="text" value="<%=data.brandind[0].parent%>"  />
              </div>
            </div>
            <div class="form-group ">
              <label for="ac_Type" class="control-label col-lg-2"> Sub AC Type</label>
              <div class="col-lg-10">
                <input class="form-control " id="ac_type" name="categoryname" type="text" value="<%=data.brandind[0].categoryname%>"/>
              </div>
            </div>
            <div class="form-group ">
              <label for="Warranty" class="control-label col-lg-2">AC Warranty_Period</label>
              <div class="col-lg-10">
                <input class="form-control " id="Warranty" name="Warranty" type="text" value="<%=data.brandind[0].wperiod%>" />
              </div>
            </div>
           
            <div class="form-group">
              <div class="col-lg-offset-2 col-lg-10">
                <a class="button_1" href="/admin/updateprdt">
                <button class="btn btn-theme" type="submit">Update</button></a>
                <button class="btn btn-theme04" type="button">Cancel</button>
              </div>
            </div>
        
          </form>
          <%}%>
        </div>
      </div>
      <!-- /form-panel -->
    </div>
    <!-- /col-lg-12 -->
  </div>
  <!-- /row -->
  </section>

        <%- include('main/foot'); -%>
    