<%- include('main/head'); -%>
<%- include('main/sidebar'); -%>
    

<!--main content start-->
<section id="main-content">
    <section class="wrapper">
      <h3><i class="fa fa-angle-right"></i> Payment of Delivery</h3>
<div class="row mt">
    <div class="col-lg-12">
      <h4><i class="fa fa-angle-right"></i> Payment of Delivery</h4>
      <div class="form-panel">
        <div class="form">
          <form class="cmxform form-horizontal style-form" id="signupForm" method="POST" action="/admin/paydelivery_individual">
            <div class="form-group ">
                <label for="pid" class="control-label col-lg-2">Id</label>
                <div class="col-lg-10">
                  <input class=" form-control" id="pid" name="pid" type="number" />
                </div>
              </div>
            <div class="form-group ">
              <label for="firstname" class="control-label col-lg-2">Firstname</label>
              <div class="col-lg-10">
                <input class=" form-control" id="firstname" name="firstname" type="text" />
              </div>
            </div>
            <div class="form-group ">
              <label for="lastname" class="control-label col-lg-2">Lastname</label>
              <div class="col-lg-10">
                <input class=" form-control" id="lastname" name="lastname" type="text" />
              </div>
            </div>
            <div class="form-group ">
              <label for="pdate" class="control-label col-lg-2">Date</label>
              <div class="col-lg-10">
                <input class="form-control " id="pdate" name="pdate" type="date" />
              </div>
            </div>
            <div class="form-group ">
              <label for="ptime" class="control-label col-lg-2">Time</label>
              <div class="col-lg-10">
                <input class="form-control " id="ptime" name="ptime" type="time" />
              </div>
            </div>
            <div class="form-group ">
              <label for="pamt" class="control-label col-lg-2">Amount</label>
              <div class="col-lg-10">
                <input class="form-control " id="pamt" name="pamt" type="number" />
              </div>
            </div>
            <div class="form-group ">
              <label for="pstatus" class="control-label col-lg-2">Status</label>
              <div class="col-lg-10">
                <input class="form-control " id="pstatus" name="pstatus" type="text" />
              </div>
            </div>
       
            
           
            <div class="form-group">
              <div class="col-lg-offset-2 col-lg-10">
                <button class="btn btn-theme" type="submit">Save</button>
                <button class="btn btn-theme04" type="button">Cancel</button>
              </div>
            </div>
          </form>
        </div>
      </div>
      <!-- /form-panel -->
    </div>
    <!-- /col-lg-12 -->
  </div>
  <!-- /row -->
  </section>
</section>
<%- include('main/foot'); -%>