<div class="row mt">
    <div class="col-lg-10">
        <br>
        <br>

      <h4 style="color:white;"><i ></i> Profile </h4><br>
      <div class="form-panel">
        <div class="form">

            <% if(data1 && data1.techcomp){%>
         
          <form class="cmxform form-horizontal style-form" id="signupForm" action="/technician/main_ser">
   
              

             <div class="form-group ">
              <label for="mtime" style="color:white;" class="control-label col-lg-5">Name</label>
              <div class="col-lg-6">
                <input class="form-control " id="cid" name="cid" type="text" value="<%=locals.technician.first_name %> <%=locals.technician.last_name %> " />
